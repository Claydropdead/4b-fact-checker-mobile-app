{
  "expo": {
    "name": "4B Fact Checker",
    "slug": "4b-fact-checker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/pnp-logo-square.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/pnp-logo-square.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/pnp-logo-square.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.claydropdead.factcheckerapp"
    },
    "web": {
      "favicon": "./assets/pnp-logo-square.png"
    },
    "extra": {
      "eas": {
        "projectId": "783c22a1-28cf-42e4-9f67-18526eb743c7"
      }
    },
    "plugins": [
      "expo-font"
    ]
  }
}
